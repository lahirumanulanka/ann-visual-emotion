        "            \n",
        "            val_loss = val_running_loss / val_total_samples\n",
        "            val_acc = val_running_corrects.double() / val_total_samples * 100\n",
        "            \n",
        "            training_history['val_losses'].append(val_loss)\n",
        "            training_history['val_accs'].append(val_acc.item())\n",
